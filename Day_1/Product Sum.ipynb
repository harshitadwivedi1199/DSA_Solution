   "source": [
    "def productSum(array,depth=1):\n",
    "    sum =0 \n",
    "    for i in array:\n",
    "        if type(i) is list:\n",
    "            sum += productSum(i,depth+1)\n",
    "        else:\n",
    "            sum += i\n",
    "    return sum * depth\n",
    "            "
   ]
